<h1>Users</h1>
<div>
    <table>
        <thead>
            <tr>
                <td>Id</td>
                <td>E-mail</td>
                <td>Role</td>
                <td>Last login</td>
            </tr>
        </thead>
        <tbody>
        <% for (var i = 0; i < data.users.length; i++) { %>
        <tr>
            <td>
                <%= data.users[i].id %>
            </td>
            <td>
                <%= data.users[i].email %>
            </td>
            <td>
                <%= data.users[i].role %>
            </td>
            <td>
                <%= data.moment(data.users[i].last_login).fromNow() %>
            </td>
        </tr>
        <% } %>
        </tbody>
    </table>
</div>